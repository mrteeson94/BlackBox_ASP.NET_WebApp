    @model blackBox.Models.Customer
@{
    ViewBag.Title = Model.Name;
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@Model.Name</h2>
<ul>
    <li>Birthday: @Model.BirthDate.Value.ToString("dd/MM/yyyy")</li>
    <li>Subscription: @Model.MembershipType.TitleMembershipType</li>
    <li>Is subscribed to newsletter: @Model.IsSubscribedToNewsletter</li>
</ul>
